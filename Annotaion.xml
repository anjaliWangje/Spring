<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
         http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- bean definitions here -->
    <context:annotation-config/>
    <bean class="AnnotationbasedAutowired.Employee" name="employees">
        <property name="name" value="Anjali"/>
        <property name="Department" value="civil"/>




    </bean>

    <bean class="AnnotationbasedAutowired.Employee" name="employees1">
        <property name="name" value=" pranali"/>
        <property name="Department" value="it"/>




    </bean>
    <bean class="AnnotationbasedAutowired.Company" name="company"/>

</beans>