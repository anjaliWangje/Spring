<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
         http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- bean definitions here -->
    <context:annotation-config/>
    <bean class="SpringLifecyleBasedonAll.Student" name="Student1" init-method="init" destroy-method="destroy">

        <property name="fees"  value="11000"/>
</bean>
    <bean class="SpringLifecyleBasedonAll.Employee" name="Employee1" >

        <property name="salary"  value="11000"/>
    </bean>
<bean class="SpringLifecyleBasedonAll.Teacher" name="teacher">
    <property name="subjectt" value="java"/>
</bean>



</beans>